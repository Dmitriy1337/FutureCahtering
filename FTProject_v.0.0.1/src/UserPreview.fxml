<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="650.0" prefWidth="1200.0" stylesheets="@img/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <ImageView fitHeight="650.0" fitWidth="1200.0" pickOnBounds="true">
         <image>
            <Image url="@img/bg.png" />
         </image>
      </ImageView>
      <BorderPane id="borderPane" fx:id="borderPane" prefHeight="650.0" prefWidth="1200.0">
         <bottom>
            <Button fx:id="startUse" mnemonicParsing="false" onAction="#startUseButtonListener" prefHeight="66.0" prefWidth="188.0" text="Начать" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="20.0" right="20.0" />
               </BorderPane.margin>
            </Button>
         </bottom>
         <top>
            <AnchorPane maxWidth="1200.0" prefHeight="550.0" prefWidth="1200.0" BorderPane.alignment="CENTER">
               <children>
                  <ListView id="topMenu" fx:id="topMenu" orientation="HORIZONTAL" prefHeight="75.0" prefWidth="1200.0">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </ListView>
                  <ImageView fitHeight="117.0" fitWidth="307.0" layoutX="423.0" layoutY="64.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@img/logomin.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="288.0" fitWidth="721.0" layoutX="230.0" layoutY="200.0" pickOnBounds="true" style="-fx-effect: dropshadow(three-pass-box, rgba(0,0,0,0.8), 10, 0, 0, 0);">
                     <image>
                        <Image url="@img/whitebg.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="47.0" fitWidth="720.0" layoutX="207.0" layoutY="178.0" pickOnBounds="true" style="-fx-effect: dropshadow(three-pass-box, rgba(0,0,0,0.8), 10, 0, 0, 0);">
                     <image>
                        <Image url="@img/whitebg.png" />
                     </image>
                  </ImageView>
                  <Label fx:id="descrName" layoutX="213.0" layoutY="177.0" prefHeight="44.0" prefWidth="704.0" styleClass="label-header" text="Name" wrapText="true">
                     <font>
                        <Font name="Calibri Bold" size="24.0" />
                     </font>
                  </Label>
                  <Label fx:id="descrText" alignment="TOP_LEFT" contentDisplay="TOP" layoutX="258.0" layoutY="250.0" prefHeight="209.0" prefWidth="668.0" text="Label" wrapText="true">
                     <font>
                        <Font name="Calibri" size="18.0" />
                     </font></Label>
                  <RadioButton layoutX="520.0" layoutY="500.0" mnemonicParsing="false" onAction="#firstDescription" />
                  <RadioButton layoutX="560.0" layoutY="500.0" mnemonicParsing="false" onAction="#secondDescription" />
                  <RadioButton layoutX="600.0" layoutY="500.0" mnemonicParsing="false" onAction="#thirdDescription" />
                  <RadioButton layoutX="640.0" layoutY="500.0" mnemonicParsing="false" onAction="#fourthDescription" />
               </children>
            </AnchorPane>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
