      </Label>
      <Label fx:id="dState" layoutX="23.0" layoutY="41.0" text="Состояние:">
         <font>
            <Font name="Calibri" size="18.0" />
         </font>
      </Label>
      <Label fx:id="dNumber" layoutX="23.0" layoutY="68.0" text="Номер заказа:">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Circle fx:id="stateCircle" fill="LIME" layoutX="275.0" layoutY="102.0" radius="8.0" stroke="BLACK" strokeType="INSIDE" />
      <Button fx:id="confirmButton" disable="true" layoutX="71.0" layoutY="81.0" mnemonicParsing="false" opacity="0.0" prefHeight="18.0" prefWidth="79.0" text="Оплачено" textAlignment="CENTER" visible="false" />
